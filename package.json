{
  "name": "simonkristiansen-website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "prettier --check --plugin-search-dir=. src/**/*.{html,js,ts,svelte}",
    "lint:fix": "prettier --write --plugin-search-dir=. src/**/*.{html,js,ts,svelte}"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^2.0.0",
    "autoprefixer": "^10.4.1",
    "postcss": "^8.4.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.8",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "sirv-cli": "^1.0.0"
  }
